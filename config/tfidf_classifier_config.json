{
    "model_name": "tfidf_classifier.joblib",
    "analyzer": "char_wb",
    "ngram_range": [3, 5],
    "min_df": 2,
    "max_df": 0.9,
    "lowercase": true,
    "sublinear_tf": true,
    "smooth_idf": true,
    "norm": "l2",
    "strip_accents": null,
    "stop_words": null,
    "n_estimators": 500,
    "learning_rate": 0.1,
    "max_depth": 6,
    "min_child_weight": 2,
    "subsample": 0.8,
    "colsample_bytree": 0.8,
    "gamma": 0.0,
    "reg_lambda": 1,
    "reg_alpha": 0,
    "objective": "multi:softmax",
    "n_jobs": -1,
    "eval_metric": "mlogloss"
}